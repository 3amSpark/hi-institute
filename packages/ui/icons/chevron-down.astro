---
interface Props {
  class?: string;
  strokeWidth?: number;
}
const { class: className = "", strokeWidth = 2.5 } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width={strokeWidth}
  stroke-linecap="round"
  stroke-linejoin="round"
  class={`${className} `}><path d="m6 9 6 6 6-6"></path></svg
>
