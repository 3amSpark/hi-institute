---
interface Props {
  class?: string;
}
const { class: className = "" } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2.5"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={`lucide lucide-check-icon lucide-check ${className}`}
  ><path d="M20 6 9 17l-5-5"></path></svg
>
